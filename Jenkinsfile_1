pipeline {
    agent any
    stages {
        stage('hello AWS') {
            steps {
                withAWS(credentials: 'aws-credentials', region: 'us-east-2') {
                    sh 'terraform init'
                    sh 'terraform plan -out=tfplan -lock=false'
                    sh 'terraform apply -auto-approve tfplan -lock=false'
                }
            }
        }
    }
}




      
